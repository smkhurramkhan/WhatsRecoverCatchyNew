<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CropVideoView">

        <attr name="guidelines">
            <enum name="off" value="0"/>
            <enum name="onTouch" value="1"/>
            <enum name="on" value="2"/>
        </attr>

        <attr name="fixAspectRatio" format="boolean"/>
        <attr name="aspectRatioX" format="integer"/>
        <attr name="aspectRatioY" format="integer"/>

    </declare-styleable>

    <declare-styleable name="TemplateView">
        <attr name="gnt_template_type" format="reference"/>
    </declare-styleable>
    <declare-styleable name="hHAdContainerClStyleable">
        <attr name="hBackgroundColor" format="reference"/>
        <attr name="hCornerRadius" format="integer"/>
        <attr name="hStrokeWidth" format="integer"/>
        <attr name="hStrokeColor" format="reference"/>
        <attr name="hLoaderContainer" format="reference"/>


    </declare-styleable>
</resources>